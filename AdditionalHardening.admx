<policyDefinitions xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   revision="1.0"
                   schemaVersion="1.0"
                   xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <policyNamespaces>
    <target prefix="AddHard"
            namespace="AddHard.Policies.AddHard" />
    <using prefix="windows"
           namespace="Microsoft.Policies.Windows" />
  </policyNamespaces>
  <resources minRequiredRevision="1.0" />
  <categories>
    <!-- Main category -->
    <category name="Cat_AddHard"
              displayName="$(string.Cat_AddHard)" />
    <!-- Subcategories -->
    <category name="Network"
              displayName="$(string.Network)">
      <parentCategory ref="Cat_AddHard" />
    </category>
    <category name="Hardening"
              displayName="$(string.Hardening)">
      <parentCategory ref="Cat_AddHard" />
    </category>
    <!-- Subsubcategories -->
    <category name="Schannel"
              displayName="$(string.Schannel)">
      <parentCategory ref="Network" />
    </category>
    <category name="SMB"
              displayName="$(string.SMB)">
      <parentCategory ref="Network" />
    </category>
    <category name="NETFrame"
              displayName="$(string.Net)">
      <parentCategory ref="Network" />
    </category>
    <category name="Protocols"
              displayName="$(string.Protocols)">
      <parentCategory ref="Schannel" />
    </category>
    <category name="WeakProtocols"
              displayName="$(string.WeakProtocols)">
      <parentCategory ref="Protocols" />
    </category>
    <category name="Ciphers"
              displayName="$(string.Ciphers)">
      <parentCategory ref="Schannel" />
    </category>
    <category name="WeakCiphers"
              displayName="$(string.WeakCiphers)">
      <parentCategory ref="Ciphers" />
    </category>
    <category name="Hashes"
              displayName="$(string.Hashes)">
      <parentCategory ref="Schannel" />
    </category>
    <category name="WeakHashes"
              displayName="$(string.WeakHashes)">
      <parentCategory ref="Hashes" />
    </category>
    <category name="KeyEx"
              displayName="$(string.KeyEx)">
      <parentCategory ref="Schannel" />
    </category>
    <category name="WeakKeyEx"
              displayName="$(string.WeakKeyEx)">
      <parentCategory ref="KeyEx" />
    </category>
  </categories>
  <policies>
    <!-- SMB section -->
    <policy name="DisableSMB1"
            class="Machine"
            displayName="$(string.DisableSMB1)"
            explainText="$(string.DisableSMB1_Explain)"
            key="SYSTEM\CurrentControlSet\Services\LanmanServer\Parameters">
      <parentCategory ref="SMB" />
      <supportedOn ref="windows:SUPPORTED_Windows_10_0" />
      <enabledList defaultKey="SYSTEM\CurrentControlSet\Services\LanmanServer\Parameters">
        <item key="SYSTEM\CurrentControlSet\Services\LanmanServer\Parameters"
              valueName="SMB1">
          <value>
            <decimal value="0" />
          </value>
        </item>
        <item key="SYSTEM\CurrentControlSet\services\mrxsmb10"
              valueName="Start">
          <value>
            <decimal value="4" />
          </value>
        </item>
      </enabledList>
    </policy>
    <!-- Schannel section -->
    <policy name="Schannel_Logging"
            class="Machine"
            displayName="$(string.SchannelLogging)"
            explainText="$(string.SchannelLogging_Explain)"
            key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL"
            valueName="EventLogging">
      <parentCategory ref="Schannel" />
      <supportedOn ref="windows:SUPPORTED_Windows7" />
      <enabledValue>
        <decimal value="7" />
      </enabledValue>
      <disabledValue>
        <decimal value="1" />
      </disabledValue>
    </policy>
    <!-- Hardening section -->
    <policy name="LSA_RunAsPPL"
            class="Machine"
            displayName="$(string.LSA_RunAsPPL)"
            explainText="$(string.LSA_RunAsPPL_Explain)"
            key="SYSTEM\CurrentControlSet\Control\Lsa"
            valueName="RunAsPPL">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows_6_3" />
      <enabledValue>
        <decimal value="1" />
      </enabledValue>
      <disabledValue>
        <decimal value="0" />
      </disabledValue>
    </policy>
    <policy name="PSLockDownPolicy"
            class="Machine"
            displayName="$(string.PSLockDownPolicy)"
            explainText="$(string.PSLockDownPolicy_Explain)"
            key="SYSTEM\CurrentControlSet\Control\Session Manager\Environment"
            valueName="__PSLockdownPolicy">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows_6_3" />
      <enabledValue>
        <string>4</string>
      </enabledValue>
      <disabledValue>
        <string>0</string>
      </disabledValue>
    </policy>
    <policy name="LoadAppInit_DLLs"
            class="Machine"
            displayName="$(string.LoadAppInit_DLLs)"
            explainText="$(string.LoadAppInit_DLLs_Explain)"
            key="Software\Microsoft\Windows NT\CurrentVersion\Windows"
            valueName="LoadAppInit_DLLs">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows7" />
      <enabledValue>
        <decimal value="1" />
      </enabledValue>
      <disabledValue>
        <decimal value="0" />
      </disabledValue>
    </policy>
    <policy name="MSCHAPv2_Iteration"
            class="Machine"
            displayName="$(string.MSCHAPv2_Iteration)"
            explainText="$(string.MSCHAPv2_Iteration_Explain)"
            presentation="$(presentation.MSCHAPv2_Iteration)"
            key="SECURITY\Cache">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows7" />
      <elements>
        <decimal id="MSCHAPv2_Iteration_count"
                 valueName="NL$IterationCount"
                 minValue="65536"
                 maxValue="200000" />
      </elements>
    </policy>
    <policy name="Pol_MSS_DisableIPSourceRouting"
            class="Machine"
            displayName="$(string.DisableIPSourceRouting)"
            explainText="$(string.DisableIPSourceRouting_Help)"
            presentation="$(presentation.DisableIPSourceRouting)"
            key="System\CurrentControlSet\Services\Tcpip\Parameters">
      <parentCategory ref="Network" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <elements>
        <enum id="DisableIPSourceRouting"
              valueName="DisableIPSourceRouting"
              required="true">
          <item displayName="$(string.DisableIPSourceRouting0)">
            <value>
              <decimal value="0" />
            </value>
          </item>
          <item displayName="$(string.DisableIPSourceRouting1)">
            <value>
              <decimal value="1" />
            </value>
          </item>
          <item displayName="$(string.DisableIPSourceRouting2)">
            <value>
              <decimal value="2" />
            </value>
          </item>
        </enum>
      </elements>
    </policy>
    <policy name="Pol_MSS_DisableIPSourceRoutingIPv6"
            class="Machine"
            displayName="$(string.DisableIPSourceRoutingIPv6)"
            explainText="$(string.DisableIPSourceRoutingIPv6_Help)"
            presentation="$(presentation.DisableIPSourceRoutingIPv6)"
            key="System\CurrentControlSet\Services\Tcpip6\Parameters">
      <parentCategory ref="Network" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <elements>
        <enum id="DisableIPSourceRoutingIPv6"
              valueName="DisableIPSourceRouting"
              required="true">
          <item displayName="$(string.DisableIPSourceRouting0)">
            <value>
              <decimal value="0" />
            </value>
          </item>
          <item displayName="$(string.DisableIPSourceRouting1)">
            <value>
              <decimal value="1" />
            </value>
          </item>
          <item displayName="$(string.DisableIPSourceRouting2)">
            <value>
              <decimal value="2" />
            </value>
          </item>
        </enum>
      </elements>
    </policy>
    <policy name="Kerberos_Logging"
            class="Machine"
            displayName="$(string.KerberosLogging)"
            explainText="$(string.KerberosLogging_Explain)"
            key="SYSTEM\CurrentControlSet\Control\Lsa\Kerberos\Parameters"
            valueName="LogLevel">
      <parentCategory ref="Network" />
      <supportedOn ref="windows:SUPPORTED_Windows7" />
      <enabledValue>
        <decimal value="1" />
      </enabledValue>
      <disabledValue>
        <decimal value="0" />
      </disabledValue>
    </policy>
    <policy name="AdminShare_Wks"
            class="Machine"
            displayName="$(string.AdminShare_Wks)"
            explainText="$(string.AdminShare_Wks_Explain)"
            key="SYSTEM\CurrentControlSet\Services\LanManServer\Parameter"
            valueName="AutoShareWks">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows_6_3" />
      <enabledValue>
        <decimal value="0" />
      </enabledValue>
      <disabledValue>
        <decimal value="1" />
      </disabledValue>
    </policy>
        <policy name="AdminShare_Srv"
            class="Machine"
            displayName="$(string.AdminShare_Srv)"
            explainText="$(string.AdminShare_Srv_Explain)"
            key="SYSTEM\CurrentControlSet\Services\LanManServer\Parameter"
            valueName="AutoShareSrv">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows_6_3" />
      <enabledValue>
        <decimal value="0" />
      </enabledValue>
      <disabledValue>
        <decimal value="1" />
      </disabledValue>
    </policy>
    <policy name="MicroarchHardening"
            class="Machine"
            displayName="$(string.MicroarchHardening)"
            explainText="$(string.MicroarchHardening_Explain)"
            presentation="$(presentation.MicroarchHardening_Presentation)"
            key="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <elements>
        <enum id="MicroarchHardening_DropdownList"
              valueName="FeatureSettingsOverride"
              required="true">
          <item displayName="$(string.MicroarchHardening_Option1)">
            <value>
              <decimal value="72" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
          <item displayName="$(string.MicroarchHardening_Option2)">
            <value>
              <decimal value="8264" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
          <item displayName="$(string.MicroarchHardening_Option3)">
            <value>
              <decimal value="8" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
          <item displayName="$(string.MicroarchHardening_Option4)">
            <value>
              <decimal value="0" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
          <item displayName="$(string.MicroarchHardening_Option5)">
            <value>
              <decimal value="1" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
          <item displayName="$(string.MicroarchHardening_Option6)">
            <value>
              <decimal value="64" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
          <item displayName="$(string.MicroarchHardening_Option7)">
            <value>
              <decimal value="3" />
            </value>
            <valueList defaultKey="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management">
              <item valueName="FeatureSettingsOverrideMask">
                <value>
                  <decimal value="3" />
                </value>
              </item>
            </valueList>
          </item>
        </enum>
        <boolean id="HyperVMitigations"
                 key="SOFTWARE\Microsoft\Windows NT\CurrentVersion\Virtualization"
                 valueName="MinVmVersionForCpuBasedMitigations">
          <trueValue>
            <string>1.0</string>
          </trueValue>
          <falseValue>
            <delete />
          </falseValue>
        </boolean>
      </elements>
    </policy>
    <policy name="SEHOP"
            class="Machine"
            displayName="$(string.SEHOP)"
            explainText="$(string.SEHOP_Explain)"
            key="SYSTEM\CurrentControlSet\Control\Session Manager\kernel"
            valueName="DisableExceptionChainValidation">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <enabledValue>
        <decimal value="0" />
      </enabledValue>
      <disabledValue>
        <decimal value="1" />
      </disabledValue>
    </policy>
    <policy name="NLA"
            class="Machine"
            displayName="$(string.NLA)"
            explainText="$(string.NLA_Explain)"
            key="SOFTWARE\Policies\Microsoft\Windows NT\Terminal Services">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <enabledList defaultKey="SOFTWARE\Policies\Microsoft\Windows NT\Terminal Services">
        <item valueName="SecurityLayer">
          <value>
            <decimal value="2" />
          </value>
        </item>
        <item valueName="UserAuthentication">
          <value>
            <decimal value="1" />
          </value>
        </item>
        <item valueName="MinEncryptionLevel">
          <value>
            <decimal value="3" />
          </value>
        </item>
      </enabledList>
    </policy>
    <policy name="LMAuth"
            class="Machine"
            displayName="$(string.LMAuth)"
            explainText="$(string.LMAuth_Explain)"
            key="SYSTEM\CurrentControlSet\Control\Lsa"
            valueName="LmCompatibilityLevel">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows_6_3" />
      <enabledValue>
        <decimal value="5" />
      </enabledValue>
      <disabledValue>
        <decimal value="1" />
      </disabledValue>
    </policy>
    <policy name="WDigest"
            class="Machine"
            displayName="$(string.WDigest)"
            explainText="$(string.WDigest_Explain)"
            key="SYSTEM\CurrentControlSet\Control\SecurityProviders\WDigest">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <enabledList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\WDigest">
        <item valueName="UseLogonCredential">
          <value>
            <decimal value="0" />
          </value>
        </item>
        <item valueName="Negotiate">
          <value>
            <decimal value="0" />
          </value>
        </item>
      </enabledList>
    </policy>
    <policy name="Logon"
            class="Machine"
            displayName="$(string.Logon)"
            explainText="$(string.Logon_Explain)"
            key="SOFTWARE\Microsoft\Windows NT\CurrentVersion\Winlogon">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <enabledList defaultKey="SOFTWARE\Microsoft\Windows NT\CurrentVersion\Winlogon">
        <item valueName="CachedLogonsCount">
          <value>
            <decimal value="3" />
          </value>
        </item>
        <item key="SYSTEM\CurrentControlSet\Control\Lsa"
              valueName="TokenLeakDetectDelaySecs">
          <value>
            <decimal value="30" />
          </value>
        </item>
      </enabledList>
    </policy>
    <policy name="ASLR"
            class="Machine"
            displayName="$(string.ASLR)"
            explainText="$(string.ASLR_Explain)"
            key="SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management"
            valueName="MoveImages">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_WindowsVista" />
      <enabledValue>
        <!-- 4294967295 = 0xFFFFFFFF -->
        <decimal value="4294967295" />
      </enabledValue>
      <disabledValue>
        <decimal value="0" />
      </disabledValue>
    </policy>
    <policy name="User32_Exception"
            class="Machine"
            displayName="$(string.User32_Exception)"
            explainText="$(string.User32_Exception_Explain)"
            key="SOFTWARE\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ALLOW_USER32_EXCEPTION_HANDLER_HARDENING">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows7" />
      <enabledList>
        <item key="SOFTWARE\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ALLOW_USER32_EXCEPTION_HANDLER_HARDENING"
              valueName="iexplore.exe">
          <value>
            <decimal value="1" />
          </value>
        </item>
        <item key="SOFTWARE\Wow6432Node\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ALLOW_USER32_EXCEPTION_HANDLER_HARDENING"
              valueName="iexplore.exe">
          <value>
            <decimal value="1" />
          </value>
        </item>
      </enabledList>
      <disabledList>
        <item key="SOFTWARE\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ALLOW_USER32_EXCEPTION_HANDLER_HARDENING"
              valueName="iexplore.exe">
          <value>
            <decimal value="0" />
          </value>
        </item>
        <item key="SOFTWARE\Wow6432Node\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ALLOW_USER32_EXCEPTION_HANDLER_HARDENING"
              valueName="iexplore.exe">
          <value>
            <decimal value="0" />
          </value>
        </item>
      </disabledList>
    </policy>
    <policy name="Info_Disclosure"
            class="Machine"
            displayName="$(string.Info_Disclosure)"
            explainText="$(string.Info_Disclosure_Explain)"
            key="SOFTWARE\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ENABLE_PRINT_INFO_DISCLOSURE_FIX">
      <parentCategory ref="Hardening" />
      <supportedOn ref="windows:SUPPORTED_Windows7" />
      <enabledList>
        <item key="SOFTWARE\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ENABLE_PRINT_INFO_DISCLOSURE_FIX"
              valueName="iexplore.exe">
          <value>
            <decimal value="1" />
          </value>
        </item>
        <item key="SOFTWARE\Wow6432Node\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ENABLE_PRINT_INFO_DISCLOSURE_FIX"
              valueName="iexplore.exe">
          <value>
            <decimal value="1" />
          </value>
        </item>
      </enabledList>
      <disabledList>
        <item key="SOFTWARE\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ENABLE_PRINT_INFO_DISCLOSURE_FIX"
              valueName="iexplore.exe">
          <value>
            <decimal value="0" />
          </value>
        </item>
        <item key="SOFTWARE\Wow6432Node\Microsoft\Internet Explorer\MAIN\FeatureControl\FEATURE_ENABLE_PRINT_INFO_DISCLOSURE_FIX"
              valueName="iexplore.exe">
          <value>
            <decimal value="0" />
          </value>
        </item>
      </disabledList>
    </policy>
    <!-- Schannel configuration section -->
    <!-- PROTOCOLS -->
    <!-- Multi-Protocol Unified Hello should always be disabled, and
             Schannel will apparently never use it. -->
    <policy name="MPUH"
            class="Machine"
            displayName="$(string.MPUH)"
            explainText="$(string.MPUH_Help)"
            presentation="$(presentation.MPUH)"
            key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello">
      <parentCategory ref="WeakProtocols" />
      <supportedOn ref="windows:SUPPORTED_Win2k" />
      <elements>
        <boolean id="MPUH_ClientCheckbox"
                 key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello\Client"
                 valueName="Enabled">
          <trueList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello\Client">
            <item valueName="Enabled">
              <value>
                <decimal value="1" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="0" />
              </value>
            </item>
          </trueList>
          <falseList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello\Client">
            <item valueName="Enabled">
              <value>
                <decimal value="0" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="1" />
              </value>
            </item>
          </falseList>
        </boolean>
        <boolean id="MPUH_ServerCheckbox"
                 key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello\Server"
                 valueName="Enabled">
          <trueList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello\Server">
            <item valueName="Enabled">
              <value>
                <decimal value="1" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="0" />
              </value>
            </item>
          </trueList>
          <falseList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\Multi-Protocol Unified Hello\Server">
            <item valueName="Enabled">
              <value>
                <decimal value="0" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="1" />
              </value>
            </item>
          </falseList>
        </boolean>
      </elements>
    </policy>
    <!-- PCT 1.0 should always be disabled, and Schannel will apparently
             never use it. -->
    <policy name="PCTv1"
            class="Machine"
            displayName="$(string.PCTv1)"
            explainText="$(string.PCTv1_Help)"
            presentation="$(presentation.PCTv1)"
            key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0">
      <parentCategory ref="WeakProtocols" />
      <supportedOn ref="windows:SUPPORTED_Win2k" />
      <elements>
        <boolean id="PCTv1_ClientCheckbox"
                 key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0\Client"
                 valueName="Enabled">
          <trueList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0\Client">
            <item valueName="Enabled">
              <value>
                <decimal value="1" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="0" />
              </value>
            </item>
          </trueList>
          <falseList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0\Client">
            <item valueName="Enabled">
              <value>
                <decimal value="0" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="1" />
              </value>
            </item>
          </falseList>
        </boolean>
        <boolean id="PCTv1_ServerCheckbox"
                 key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0\Server"
                 valueName="Enabled">
          <trueList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0\Server">
            <item valueName="Enabled">
              <value>
                <decimal value="1" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="0" />
              </value>
            </item>
          </trueList>
          <falseList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\PCT 1.0\Server">
            <item valueName="Enabled">
              <value>
                <decimal value="0" />
              </value>
            </item>
            <item valueName="DisabledByDefault">
              <value>
                <decimal value="1" />
              </value>
            </item>
          </falseList>
        </boolean>
      </elements>
    </policy>
    <!-- SSLv2 should always be disabled. -->
    <policy name="SSLv2"
            class="Machine"
            displayName="$(string.SSLv2)"
            explainText="$(string.SSLv2_Help)"
            presentation="$(presentation.SSLv2)"
            key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\SSL 2.0">
      <parentCategory ref="WeakProtocols" />
      <supportedOn ref="windows:SUPPORTED_Win2k" />
      <elements>
        <boolean id="SSLv2_ClientCheckbox"
                 key="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\SSL 2.0\Client"
                 valueName="Enabled">
          <trueList defaultKey="SYSTEM\CurrentControlSet\Control\SecurityProviders\SCHANNEL\Protocols\SSL 2.0\Client">
